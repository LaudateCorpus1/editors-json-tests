{
  "description": "Tests code completion in non whitelisted file",
  "live_environment": false,
  "setup": {
    "kited": "authenticated"
  },
  "test": [
    {
      "description": "opening a file",
      "step": "action",
      "type": "open",
      "properties": {
        "file": "data/whitelisted/file.py"
      }
    },
    {
      "description": "moving the cursor at the end",
      "step": "action",
      "type": "move_cursor",
      "properties": {
        "offset": 11
      }
    },
    {
      "description": "typing unicode characters",
      "step": "action",
      "type": "input_text",
      "properties": {
        "text": "\n你好"
      }
    },
    {
      "description": "and typing an identifier",
      "step": "action",
      "type": "input_text",
      "properties": {
        "text": "\njson.dum"
      }
    },
    {
      "description": "does not send a completion request",
      "step": "expect_not",
      "type": "request",
      "properties": {
        "path": "/clientapi/editor/completions",
        "method": "POST",
        "body": {
          "editor": "${plugin}",
          "filename": "${editors.data/whitelisted/file.py.filename}",
          "text": "import json\n你好\njson.dum",
          "cursor_runes": 23
        }
      }
    }
  ]
}
